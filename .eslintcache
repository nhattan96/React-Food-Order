rtHandler = () => {\n    setCartIsShown((prev) => !prev)\n  }\n\n  return (\n    // <Fragment>\n    <CartProvider>\n      {cartIsShown && <Cart onClose={showCartHandler}></Cart>}\n      <Header onShownCart={showCartHandler}></Header>\n      <main>\n        <Meals></Meals>\n      </main>\n\n    </CartProvider>\n    // </Fragment>\n  );\n}\n\nexport default App;\n","/Users/nhattan/Documents/Learn/React-TheComplete-Guide-Udemy/React-Food-Order/src/components/Layout/Header/Header.js",[],"/Users/nhattan/Documents/Learn/React-TheComplete-Guide-Udemy/React-Food-Order/src/components/Layout/Header/HeaderCartButton.js",[],"/Users/nhattan/Documents/Learn/React-TheComplete-Guide-Udemy/React-Food-Order/src/components/Cart/CartIcon.js",[],"/Users/nhattan/Documents/Learn/React-TheComplete-Guide-Udemy/React-Food-Order/src/components/Meals/Meals.js",[],"/Users/nhattan/Documents/Learn/React-TheComplete-Guide-Udemy/React-Food-Order/src/components/Meals/AvailableMeals.js",[],"/Users/nhattan/Documents/Learn/React-TheComplete-Guide-Udemy/React-Food-Order/src/components/Meals/MealsSummary.js",[],"/Users/nhattan/Documents/Learn/React-TheComplete-Guide-Udemy/React-Food-Order/src/components/UI/Card.js",[],"/Users/nhattan/Documents/Learn/React-TheComplete-Guide-Udemy/React-Food-Order/src/components/Meals/MeatItem/MealItem.js",[],"/Users/nhattan/Documents/Learn/React-TheComplete-Guide-Udemy/React-Food-Order/src/components/Meals/MeatItem/MealItemForm.js",["74"],"import { useState, useContext, useRef } from 'react'\nimport Input from '../../UI/Input'\nimport classes from './MealItemForm.module.css'\n\nconst MealItemForm = (props) => {\n    const [amountIsValid, setAmountIsValid ] = useState(true)\n    const amountInputRef = useRef()\n\n    const submitHandler = event => {\n        event.preventDefault()\n\n        const enteredAmount = amountInputRef.current.value // always String\n\n        const enteredAmountNumber = +enteredAmount\n\n        if (\n            enteredAmount.trim().length === 0 ||\n            enteredAmountNumber < 1 ||\n            enteredAmountNumber > 5\n        ) {\n            setAmountIsValid(false)\n            return\n        }\n\n        props.onAddToCart(enteredAmountNumber)\n    }\n\n    return (\n        <form className={classes.form} onSubmit={submitHandler}>\n            <Input\n                ref={amountInputRef}\n                label=\"Amount\"\n                input={{\n                    id: 'amount_' + props.id,\n                    type: 'number',\n                    min: '1',\n                    max: '5',\n                    step: '1',\n                    defaultValue: '1'\n                }}></Input>\n            <button>+ Add</button>\n            { !amountIsValid && <p>Please enter a valid amount (1-5)</p>}\n        </form>\n    )\n\n}\n\nexport default MealItemForm","/Users/nhattan/Documents/Learn/React-TheComplete-Guide-Udemy/React-Food-Order/src/components/UI/Input.js",[],"/Users/nhattan/Documents/Learn/React-TheComplete-Guide-Udemy/React-Food-Order/src/components/Cart/Cart.js",[],"/Users/nhattan/Documents/Learn/React-TheComplete-Guide-Udemy/React-Food-Order/src/components/UI/Modal.js",["75"],"import react from 'react'\nimport { Fragment } from 'react'\nimport ReactDOM from 'react-dom'\nimport classes from './Modal.module.css'\n\n\nconst Backdrop = (props) => {\n    return (\n        <div className={classes.backdrop} onClick={props.onClose}>\n\n        </div>\n    )\n}\n\nconst ModalOverlay = (props) => {\n    return (\n        <div className={classes.modal}>\n            <div className={classes.content}>\n                {props.children}\n            </div>\n        </div>\n    )\n}\n\nconst portalElement = document.getElementById('overlays')\n\nconst Modal = (props) => {\n    return <Fragment>\n        {ReactDOM.createPortal((<Backdrop onClose={props.onClose}></Backdrop>), portalElement)}\n        {ReactDOM.createPortal((<ModalOverlay>{props.children}</ModalOverlay>), portalElement)}\n    </Fragment>\n}